<h1>Enviar Arquivos</h1>
<hr />
<div class="jumbotron jumbotron-fluid">
    <div class="container">
        <div class="row">
            <section>
                <form method="post" enctype="multipart/form-data" asp-action="Upload">
                    <div class="form-group">
                        <div class="col-md-10">
                            <p>Faça o upload de um ou mais arquivos</p>
                            <input type="file" name="files" multiple />
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-md-10">
                            <input class="btn btn-success" type="submit" value="Enviar arquivo(s)" />
                        </div>
                    </div>
                </form>
            </section>
        </div>
    </div>
</div>
<a asp-action="Index" asp-controller="Admin">Voltar a página inicial</a>
